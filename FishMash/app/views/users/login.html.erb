<h2>Sign in</h2>
<%= form_for :user, url: authenticate_users_path do |f| %>
  <div class="form-group">
    <%= f.label :login %>
    <%= f.text_field :login %>
  </div>
  <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div class="form-group">
    <%= f.submit "Login" %>
  </div>
<% end %>
