<div class="col-md-6">
  <%= form_for @student_class, url: class_path(@student_class) do |f| %>
    <p>
      <%= f.label :name %>
      <%= f.text_field :name, placeholder: :name %>
    </p>
    <p>
      <%= f.label :description %>
      <%= f.text_area :description, placeholder: :description %>
    </p>
    <p>
      <%= f.submit "Save changes" %>
    </p>
  <% end %>
</div>
<div class="col-md-6">
  <h2>Members</h2>
</div>
